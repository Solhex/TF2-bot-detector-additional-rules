		},
		{
			"actions": {
				"mark": [
					"racist"
				]
			},
			"description": "Racistism Detected",
			"triggers": {
				"chatmsg_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"\\w*n[i1]gg[e3]r\\w*",
						"\\s*n[i1]gg[e3]r[s5]?\\s*",
						"n[i1]gg[e3]r[s5]?",
						"n[i1]gr[o0][s5]?"
					]
				}
			}
		}
	]
