			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "braaaap / shoppy Bots",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "3e28feb1e131d1724f94cbf486edd721ccd459a0"
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"(Scout|Soldier|Pyro|Demo|Heavy|Engie|Medic|Sniper|Spy) Gaming (\\((REAL|NOT A BOT)\\))$",
						"((spoooky )?braaaap \\d+)$",
						"(shoppy.gg/@d3fc0n6)$"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Waffen-SS Bots",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"(\u200f*W\u200f*a\u200f*f\u200f*f\u200f*e\u200f*n\u200f*-\u200f*S\u200f*S\u200f*)$"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "m4gic bots",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"(m4gic)$"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Sydney Bots",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"((?!Sydney)S[\u0443y]dn[\u0435e][\u0443y]( ?\\| ?bit.ly\\/\\w*)?)$"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater",
					"racist"
				]
			},
