			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Additional Namestealing Bots Rules",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"\u202d",
						"\u202e",
						"\u202f",
						"\u205f",
						"\u0e4a",
						"\u0e4b",
