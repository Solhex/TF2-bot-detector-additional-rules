			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Nemo Bots",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"((OG )?Fuck (\u0e4a\u202d)?Nemo(\u0e4a\u202d)? ?(v2 #\\d+)?(#FuckNemoV2Update)?)$"
					]
