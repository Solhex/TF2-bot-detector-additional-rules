{
	"$schema": "https://raw.githubusercontent.com/PazerOP/tf2_bot_detector/master/schemas/v3/rules.schema.json",
	"file_info": {
		"authors": [
			"Solhex"
		],
		"description": "Solhex's rules list for TF2 Bot Detector, most of the code used here is from Link2006's and moebkun's rule list (please use theirs as I guarantee its way better) which are marked with their username.",
		"title": "Modified rules list",
		"update_url": "https://gist.githubusercontent.com/Solhex/1f24132b235dcb9daffb2ebdc1d4769c/raw/rules.solhex.json"
	},
	"rules": [
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Avatar and Text Match",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "18541ade658c70e7d26211eb08e4bf394810bb87"
					}
				],
				"chatmsg_text_match": {
					"case_sensitive": false,
					"mode": "ends_with",
					"patterns": [
						"https://matrix.to/#/#bot_rollers:tchncs.de"
					]
				},
				"mode": "match_any"
			}
		},
		{
			"actions": {
				"mark": [
					"suspicious"
				]
			},
			"description": "King of I Love You",
			"triggers": {
				"chatmsg_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"https://steamcommunity.com/groups/snipermainunited"
					]
				},
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"\u2665King of \"\u2665I Love You\u2665\" b"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater",
					"exploiter"
				]
			},
			"description": "mattservice Bots",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "da0346b6a074bab19e6743eaa4805d4b4ef28820"
					},
					{
						"avatar_hash": "c38eea8519e48a1e385c58ba770a249e3657b46c"
					}
				],
				"chatmsg_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"HOW TO ADD: Steam->Community->Find People-> Matt Gift!"
					]
				},
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"(Matt ((is Donating!)|(Gifts?!)|(Giveaway!))( \\[BOT\\])?)$"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Crazy Frog bots",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"((french|racist)? ?((cr?[a@]?z?[ye])|annoying) ?f[ro]+g*!?( in da house)?)$",
						"(Tiny)? Frog Crazy",
						"Welcome to Crazy Fortress 2!",
						"A Blast From The Past"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "DoesHotter bots",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "69eea37cc416c5ba389b276335798c1111bebbef"
					}
				],
				"mode": "match_all",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "ends_with",
					"patterns": [
						"DoesHotter"
					]
				}
			}
		},
