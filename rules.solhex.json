{
	"$schema": "https://raw.githubusercontent.com/PazerOP/tf2_bot_detector/master/schemas/v3/rules.schema.json",
	"file_info": {
		"authors": [
			"Solhex"
		],
		"description": "Solhex's rules list for TF2 Bot Detector, most of the code used here is from Link2006's and moebkun's rule list (please use theirs as I guarantee its way better) which are marked with their username.",
		"title": "Modified rules list",
		"update_url": "https://raw.githubusercontent.com/Solhex/TF2-bot-detector-additional-rules/main/rules.solhex.json"
	},
	"rules": [
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Avatar Match",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "18541ade658c70e7d26211eb08e4bf394810bb87"
					}
				]
				}
		},
		{
			"actions": {
				"mark": [
					"cheater",
					"exploiter"
				]
			},
			"description": "mattservice",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "da0346b6a074bab19e6743eaa4805d4b4ef28820"
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"Matt is Donating!",
						"Matt Gifts!",
						"Matt Gift!"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "snooker",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "b2824852210eab28c9c6307111f4b0fcff80c07f"
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"matchroom.live/snooker"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "braaaap",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "3e28feb1e131d1724f94cbf486edd721ccd459a0"
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"braaaap \\d+",
						"spoooky braaaap \\d+"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "crusader",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "b957a547cca39078dbab8d6ed3b5171af25ecaff"
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"GYPSY CRUSADER"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater",
					"racist"
				]
			},
			"description": "NK",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "a03023b2361fb9aa7a94cf515425236255844c75"
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"^\\[VALVE\\]\u5350?N[\u00edi]gg[\\de]rkill[\\de]r\u534d?"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater",
					"racist"
				]
			},
			"description": "[Link2006] NK",
			"triggers": {
				"avatar_match": [
					{
						"avatar_hash": "c35571183a64018d1eff01be6867cde37c2d5909"
					}
				],
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"\\x4E\\x69\\x67\\x67\\x65\\x72killer",
						"TF2 PLAYER = APE",
						"\/id\/heinrichderpro"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "[Link2006] Lmaobox advert",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"WWW.LMAOBOX.NET"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "[Link2006] 'Sydney' bots (Does not include 'Sydney' due to false-positives)",
			"triggers": {
				"mode": "match_any",
				"username_text_match": {
					"case_sensitive": true,
					"mode": "contains",
					"patterns": [
						"Totally Not An Annoying Pony",
						"Milenko Gaming",
						"The Great Milenko",
						"S\u0443dney",
						"S\u0443dne\u0443"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "[Link2006] NameSteal2",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"\u205f",
						"\u200f",
						"\u200e",
						"\u200d",
						"\u200c",
						"\u200b",
						"\u2060",
						"\u202a",
						"\u202c",
						"\u202d",
						"\u2061",
						"\ufeff",
						"\u0e4a",
						"\u0e4e",
						"\ufff9"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "[Link2006] Broken Namesteal bots",
			"triggers": {
				"username_text_match": {
					"case_sensitive": true,
					"mode": "contains",
					"patterns": [
						"\\ u202C"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"racist"
				]
			},
			"description": "[Moeb]Racistism detected",
			"triggers": {
				"chatmsg_text_match": {
					"case_sensitive": false,
					"mode": "regex",
					"patterns": [
						"\\w*N\udb40\udc21\udb40\udc21IGGE\udb40\udc21\udb40\udc21R\\w*",
						"\\w*\u0274\u026a\u0262\u0262\u1d07\u0280\\w*",
						"\\w*n\u0456gg\u0435r\\w*",
						"\\w*N\u00cdGGER\\w*",
						"\\w*nigg\u0435r\\w*",
						"\\w*N1GG3R\\w*",
						"\\w*nigger\\w*",
						"\\w*nigg4\\w*",
						"\\w*niggz\\w*",
						"niggers"
					]
				},
				"mode": "match_any"
			}
		}
	]
}